@model List<SkypeChatGame.Models.GameModel>
@{
    ViewBag.Title = "Chat";
}

<div id="video-meta">
    <h6></h6>
</div>
<div id="slider-vertical">
</div>
@{
    //CHANGE DB TO LOCAL AND CHECK THAT THIS WORK SFOR GILLER2
    if(Model != null != null && Model.Count > 0 && Model.ElementAt(0).Player != null)
    {
    var m = Model.Find(f => f.Player.UserName.ToLower() ==WebSecurity.CurrentUserName.ToLower());
 if(m != null){   
     <div id="mute-button"><img src="~/images/mute.png" /></div>
 }
 }
}
<div id="gameBody">
    <h3 class="center-text" id="time-left"></h3>
    <h3 class="center-text" id="round">Click the start button to start a new game.</h3>
    <div id="question-wrapper" class="center-text">
        <radiogroup id="discussion">
        </radiogroup>
    </div>
    <div id="button-wrapper" class="center-text">
        <input type="button" class="button-style" id="sendmessage" value="Start" />
        <input class="button-style hidden" type="button" id="stoptimerbutton" value="Send" />
    </div>
</div>
<div id="player"></div>
@section scripts {
    <!--Script references. 
<!--The jQuery library is required and is referenced by default in _Layout.cshtml. -->
<!--Reference the SignalR library. -->
       <script src="~/bundles/jqueryui"></script> 
<script src="~/Scripts/jquery.signalR-2.1.1.min.js"></script>
<!--Reference the autogenerated SignalR hub script. -->
<script src="~/signalr/hubs"></script>
<!--SignalR script to update the chat page and send messages.-->
    <script src="http://www.youtube.com/iframe_api"></script>
<script src="~/Content/js/game.js">
</script>
}